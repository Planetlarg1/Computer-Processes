/**
 * FZero.hdl
 * Logic drawn from turth table FZero: (f1, f0) = (1, 1).
 */

CHIP FZero {
    IN A, B, C, D;
    OUT F, G;

    PARTS:
    // F
    AND(a=A, b=true, out=And1);
    AND(a=B, b=true, out=And2);
    OR(a=And1, b=And2, out=F);

    // G
    XOR(a=A, b=B, out=Xor1);
    XOR(a=Xor1, b=C, out=Xor2);
    XOR(a=Xor2, b=D, out=G);
}