/**
 * FOne.hdl
 * Logic drawn from turth table FOne: (f1, f0) = (1, 0).
 */

CHIP FOne {
    IN A, B, C, D; // Inputs
    OUT F, G; // Outputs

    PARTS:
    // F
    AND(a=A, b=false, out=And1);
    AND(a=B, b=false, out=And2);
    AND(a=C, b=D, out=And3);
    OR(a=And1, b=And2, out=F);

    // G
    AND(a=B, b=false, out=And4);
    AND(a=D, b=false, out=And5);
    OR(a=And4, b=And5, out=G);
}
