/**
 * FThree.hdl
 * Logic drawn from turth table FThree: (f1, f0) = (0, 0).
 */

CHIP FThree {
    IN A, B, C, D, E; // INputs
    OUT E, F, G; // Outputs

    PARTS:
    // E
    NOT(in=E, out=NotE);
    OR(a=NotE, b=C, out=E);

    // F
    AND(a=B, b=D, out=And1);
    OR(a=And1, b=E, out=F);

    // G
    AND(a=A, b=C, out=And2);
    OR(a=And2, b=B, out=G);
}
